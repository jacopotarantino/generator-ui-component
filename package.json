{
  "name": "generator-ui-component",
  "version": "2.2.0",
  "description": "Scaffolds a UI Component with ES6, tests, and webpack+babel build.",
  "homepage": "https://jack.ofspades.com",
  "author": {
    "name": "Jack",
    "email": "jack@ofspades.com",
